# Node.js + TypeScript .gitignore

# Ignore node_modules directory
node_modules/

# Ignore build output
dist/
out/
build/

# Ignore TypeScript build info
*.tsbuildinfo

# Ignore logs and npm debug log
logs/
*.log
npm-debug.log*

# Ignore environment variables file
.env

# Ignore .DS_Store files (macOS)
.DS_Store

# Ignore VSCode settings
.vscode/

# Ignore package-lock.json for npm
package-lock.json

# Ignore Yarn lock file
yarn.lock

# Ignore editor-specific files
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln
